{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:54176",
      "sslPort": 44381
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        //Environment variable dev | qa | uat | prod
        "Environment": "dev",
        "ASPNETCORE_ENVIRONMENT": "Development",
        // Databases
        "devBD": "Server=cmv8104;Database=HAPE;",
        "qaBD": "Server=cmv8020;Database=HAPE;",
        "uatBD": "Server=cmv8020\\uat;Database=HAPE;",
        "prodBD": "Server=cmv8020;Database=HAPE;",
        // Logs
        "devServiceLog": "C:\\CoreLogs\\dev\\ServiceLog.log",
        "qaServiceLog": "C:\\CoreLogs\\qa\\ServiceLog.log",
        "uatServiceLog": "C:\\CoreLogs\\uat\\ServiceLog.log",
        "prodServiceLog": "C:\\CoreLogs\\ApiService\\ServiceLog.log"
      }
    },
    "cmv.tecnologia.ApiService": {
      "commandName": "Project",
